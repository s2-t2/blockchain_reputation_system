@misc{Bergquist1107612,
   author = {Bergquist, Jonatan},
   institution = {Uppsala University, Division of Computing Science},
   note = {Presentation held externally at Technical University Munich on the 29-5-2017 10.50.},
   pages = {62},
   school = {Uppsala University, Division of Computing Science},
   title = {Blockchain Technology and Smart Contracts: Privacy-preserving Tools},
   series = {UPTEC F},
   ISSN = {1401-5757},
   number = {17023},
   keywords = {blockchain, smart contracts},
   abstract = {The purpose of this Master's thesis is to explore blockchain technology and smart contracts as a way of building privacy-sensitive applications. The main focus is on a medication plan containing prescriptions, built on a blockchain system of smart contracts. This is an example use case, but the results can be transferred to other ones where sensitive data is being shared and a proof of validity or authentication is needed. First the problem is presented, why medication plans are in need of digitalisation and why blockchain technology is a fitting technology for implementing such an application. Then blockchain technology is explained, since it is a very new and relatively unfamiliar IT construct. Thereafter, a design is proposed for solving the problem. A system of smart contracts was built to prove how such an application can be built, and suggested guidelines for how a blockchain system should be designed to fulfil the requirements that were defined. Finally, a discussion is held regarding the applicability of different blockchain designs to the problem of privacy-handling applications. },
   year = {2017}
}
